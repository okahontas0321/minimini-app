<div class="contents row">
    <%= form_with(model: @blog, local: true ) do %>
      <h3>
        編集する
      </h3>
      <input placeholder="Image Url" type="text" name="image" value="<%= @blog.image %>" autofocus="true">
      <textarea cols="30" name="text" placeholder="text" rows="10"><%= @blog.text %></textarea>
      <input type="submit" value="SENT">
    <% end %>
  </div>
